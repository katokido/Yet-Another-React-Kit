{
  "name": "Yet Another React Kit",
  "version": "0.0.2",
  "description": "Yet Another React Kit Demo",
  "main": "bin/server",
  "bin": {
    "build": "./bin/build.js"
  },
  "engines": {
    "node": ">=4.5.0",
    "npm": "^3.0.0"
  },
  "config": {
    "port": "3030"
  },
  "scripts": {
    "clean": "rimraf dist",
    "compile": "better-npm-run compile",
    "lint": "eslint bin build config core models server simulator src tests",
    "lint:fix": "npm run lint -- --fix",
    "start": "better-npm-run start",
    "dev": "better-npm-run dev",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "deploy:dev": "better-npm-run deploy:dev",
    "deploy:prod": "better-npm-run deploy:prod",
    "diff": "git diff --color | diff-so-fancy",
    "host": "better-npm-run host",
    "codecov": "cat coverage/*/lcov.info | codecov"
  },
  "betterScripts": {
    "host": {
      "command": "npm run clean && node bin/compile $host",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "compile": {
      "command": "node bin/compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "dev": {
      "command": "nodemon bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run clean && npm run compile",
      "env": {
        "DEBUG": "app:*"
      }
    },
    "deploy:dev": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy:prod": {
      "command": "npm run deploy",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "node bin/server",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "author": "K.K",
  "license": "MIT",
  "dependencies": {
    "antd": "^2.10.1",
    "axios": "^0.16.1",
    "classnames": "^2.2.5",
    "console-polyfill": "^0.3.0",
    "g2": "^2.3.1",
    "g2-react": "^1.2.0",
    "history": "^4.0.0",
    "intl": "^1.2.5",
    "ip": "^1.1.2",
    "js2excel": "^0.3.5",
    "lodash": "^4.17.4",
    "n2c": "^1.0.13",
    "node-sass": "^4.0.0",
    "normalize.css": "^7.0.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.5.8",
    "react": "^15.0.0",
    "react-dom": "^15.0.0",
    "react-draft-wysiwyg": "^1.9.9",
    "react-redux": "^5.0.1",
    "react-router": "^3.0.0",
    "react-spinkit": "^2.1.2",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.23.1",
    "babel-eslint": "^7.1.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-import": "^1.1.0",
    "babel-plugin-istanbul": "^3.0.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.3.13",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "^0.0.15",
    "chai": "^3.4.1",
    "chai-as-promised": "^6.0.0",
    "chai-enzyme": "^0.6.1",
    "cheerio": "^0.22.0",
    "codecov": "^2.1.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.1",
    "cssnano": "^3.7.4",
    "debug": "^2.2.0",
    "detect-port": "^1.1.2",
    "enzyme": "^2.0.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^6.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "^4.0.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-babel": "^4.0.0",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-node": "^5.0.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^6.0.0",
